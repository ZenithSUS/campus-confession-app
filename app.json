{
  "expo": {
    "name": "campus-confession-app",
    "slug": "campus-confession-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ZenithConfession.png",
    "scheme": "campusconfessionapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/ZenithConfession.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "export",
      "favicon": "./assets/images/ZenithConfession.png"
    },
    "experimental": {
      "router": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/ZenithConfession.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0dcf6d42-2677-4449-b409-f6d7cc9eaa28"
      }
    }
  }
}
