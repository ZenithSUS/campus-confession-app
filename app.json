{
  "expo": {
    "name": "Zenith Confession",
    "slug": "zenith-confession-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ZenithConfession.png",
    "scheme": "zenithconfessionapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/ZenithConfession.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zenithsus.campusconfessionapp",
      "versionCode": 1,
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"]
    },
    "web": {
      "bundler": "metro",
      "output": "export",
      "favicon": "./assets/icons/ZenithConfession.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/ZenithConfession.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      "expo-head",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "zenith-campus-confession",
          "organization": "zenithsus"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2a9ce1bf-2c5f-44bf-8bdb-27c875dfd11a"
      }
    },
    "owner": "zenithsus"
  }
}
